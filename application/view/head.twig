<meta charset="utf-8">
<title>{{ title }}</title>

<link href="{{ base_url }}/styles/{{ site }}.less" rel="stylesheet/less" type="text/css">
<script src="http://cdn.polyfill.io/v1/polyfill.min.js" async></script>
<script src="{{ base_url }}/scripts/less.js" type="text/javascript" async></script>
{% if site == 'feedback' %}
    <script src="{{ base_url }}/scripts/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="{{ base_url }}/scripts/owl.carousel.js" type="text/javascript"></script>
    <script src="{{ base_url }}/scripts/{{ site }}.js" type="text/javascript"></script>
{% endif %}

<link rel="shortcut icon" href="{{ base_url }}/images/favicon.ico" type="image/x-icon" />
